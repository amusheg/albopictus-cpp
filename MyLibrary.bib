@article{ryabovVirulentStrainDeformed2014,
  title = {A {{Virulent Strain}} of {{Deformed Wing Virus}} ({{DWV}}) of {{Honeybees}} ({{Apis}} Mellifera) {{Prevails}} after {{Varroa}} Destructor-{{Mediated}}, or {{In Vitro}}, {{Transmission}}},
  author = {Ryabov, Eugene V. and Wood, Graham R. and Fannon, Jessica M. and Moore, Jonathan D. and Bull, James C. and Chandler, Dave and Mead, Andrew and Burroughs, Nigel and Evans, David J.},
