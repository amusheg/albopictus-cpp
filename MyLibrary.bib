}

@article{vejzagicBacteriainducedEggHatching2015,
  title = {Bacteria-Induced Egg Hatching Differs for {{Trichuris}} Muris and {{Trichuris}} Suis},
  author = {Vejzagi{\'c}, Nermina and Adelfio, Roberto and Keiser, Jennifer and Kringel, Helene and Thamsborg, Stig Milan and Kapel, Christian M.O.},
  year = {2015},
