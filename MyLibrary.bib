@article{shinFirstMicrobialEnvironment2015,
  title = {The First Microbial Environment of Infants Born by {{C}}-Section: The Operating Room Microbes},
  shorttitle = {The First Microbial Environment of Infants Born by {{C}}-Section},
  author = {Shin, Hakdong and Pei, Zhiheng and Martinez, Keith A. and {Rivera-Vinas}, Juana I. and Mendez, Keimari and Cavallin, Humberto and {Dominguez-Bello}, Maria G.},
  year = {2015},
