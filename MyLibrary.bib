  title = {Burkholderia Gut Symbiont Modulates Titer of Specific Juvenile Hormone in the Bean Bug {{Riptortus}} Pedestris},
  author = {Lee, Junbeom and Kim, Chan-Hee and Jang, Ho Am and Kim, Jiyeun Kate and Kotaki, Toyomi and Shinoda, Tetsuro and Shinada, Tetsuro and Yoo, Jin-Wook and Lee, Bok Luel},
