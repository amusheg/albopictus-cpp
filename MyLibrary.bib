@techreport{joschinskiDiapauseNotSelected2019,
  title = {Diapause Is Not Selected as a Bet-Hedging Strategy in Insects: A Meta-Analysis of Reaction Norm Shapes},
  shorttitle = {Diapause Is Not Selected as a Bet-Hedging Strategy in Insects},
  author = {Joschinski, Jens and Bonte, Dries},
  year = {2019},
  month = sep,
  institution = {{Ecology}},
  doi = {10.1101/752881},
  abstract = {Many organisms escape from lethal climatological conditions by entering a resistant resting stage called diapause, and it is essential that this strategy remains optimally timed with seasonal change. Climate change therefore exerts selection pressure on phenology, which is expected to cause the evolution of mean diapause timing, but also phenotypic plasticity and bet-hedging strategies. Especially the latter as a strategy to cope with unpredictability is so far little considered in the context of climate change, and it is unknown whether it can readily evolve.},
  file = {/Users/sashamushegian/Zotero/storage/4B9JE8M4/752881v2.full.pdf;/Users/sashamushegian/Zotero/storage/WNQ8VQXT/Joschinski and Bonte - 2019 - Diapause is not selected as a bet-hedging strategy.pdf},
  language = {en},
  type = {Preprint}
}

@techreport{joschinskiTransgenerationalPlasticityBethedging2019,
  title = {Transgenerational Plasticity and Bet-Hedging: A Framework for Reaction Norm Evolution in a Rapidly Changing Climate},
  shorttitle = {Transgenerational Plasticity and Bet-Hedging},
  author = {Joschinski, Jens and Bonte, Dries},
  year = {2019},
  month = jun,
  institution = {{EcoEvoRxiv}},
  doi = {10.32942/osf.io/trg34},
  abstract = {Organisms are typically assumed to respond to environmental change by genetic adaptation or by phenotypic plasticity. While genetic tracking is potentially too slow to keep up with climate change, plasticity allows rapid responses to fluctuation and may moreover accelerate adaptation by genetic accommodation. However, phenotypic plasticity relies on environmental cues, and especially in the case of trans-generational plasticity these cues are imperfect indicators of future change. Decision-making under uncertain conditions favors diversified bet-hedging, a strategy of spreading risks by investing in a range of offspring phenotypes rather than a single optimal phenotype. Despite their close similarity, trans-generational plasticity and bet-hedging remain poorly connected, and the role of bet-hedging in adapting to climate change is rarely considered. We here develop a unifying framework: based on traits with binary outcomes (e.g. seed germination; wing polyphenism; diapause incidence) we clarify that diversified bet-hedging and trans-generational plasticity are mutually exclusive strategies, arising from opposing changes in reaction norms (allocating phenotypic variance among or within environments). Since these two strategies have in common that they shape phenotypic variance within populations, both may determine evolutionary dynamics and hence resilience to climate change. We advocate that a paradigm shift is required to accommodate the role of bet-hedging in evolution.},
  file = {/Users/sashamushegian/Zotero/storage/WGZHNUF8/Joschinski and Bonte - 2019 - Transgenerational plasticity and bet-hedging a fr.pdf},
  language = {en},
  type = {Preprint}
}

@article{julianoEcologyInvasiveMosquitoes2005,
