@article{kangGenomicDivergenceAdaptive2019,
  title = {Genomic Divergence and Adaptive Convergence in {{{\emph{Drosophila}}}}{\emph{ Simulans}} from {{Evolution Canyon}}, {{Israel}}},
  author = {Kang, Lin and Rashkovetsky, Eugenia and Michalak, Katarzyna and Garner, Harold R. and Mahaney, James E. and Rzigalinski, Beverly A. and Korol, Abraham and Nevo, Eviatar and Michalak, Pawel},
  year = {2019},
  month = may,
