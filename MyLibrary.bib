  type = {Preprint}
}

@article{hernandez-de-diegoEBioKitStandaloneEducational2017,
  title = {The {{eBioKit}}, a Stand-Alone Educational Platform for Bioinformatics},
  author = {{Hern{\'a}ndez-de-Diego}, Rafael and {de Villiers}, Etienne P. and Klingstr{\"o}m, Tomas and Gourl{\'e}, Hadrien and Conesa, Ana and {Bongcam-Rudloff}, Erik},
