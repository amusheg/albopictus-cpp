  shorttitle = {An Integrative Approach to Understanding Microbial Diversity},
  author = {Gudelj, Ivana and Weitz, Joshua S. and Ferenci, Tom and {Claire Horner-Devine}, M. and Marx, Christopher J. and Meyer, Justin R. and Forde, Samantha E.},
  year = {2010},
