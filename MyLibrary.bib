@article{evansCarryoverEffectsUrban2018,
  title = {Carry-over Effects of Urban Larval Environments on the Transmission Potential of Dengue-2 Virus},
  author = {Evans, Michelle V. and Shiau, Justine C. and Solano, Nicole and Brindley, Melinda A. and Drake, John M. and Murdock, Courtney C.},
  year = {2018},
  month = dec,
  volume = {11},
