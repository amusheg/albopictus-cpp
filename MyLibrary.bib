@article{schmidtGEOGRAPHICVARIATIONDIAPAUSE2005,
  title = {{{GEOGRAPHIC VARIATION IN DIAPAUSE INCIDENCE}}, {{LIFE}}-{{HISTORY TRAITS}}, {{AND CLIMATIC ADAPTATION IN DROSOPHILA MELANOGASTER}}},
  author = {Schmidt, Paul S. and Matzkin, Luciano and Ippolito, Michael and Eanes, Walter F.},
  year = {2005},
