@article{salemActinobacteriaEssentialSymbionts2013,
  title = {Actinobacteria as Essential Symbionts in Firebugs and Cotton Stainers ({{Hemiptera}}, {{Pyrrhocoridae}}): {{Actinobacteria}} as Essential Symbionts in Firebugs and Cotton Stainers},
  shorttitle = {Actinobacteria as Essential Symbionts in Firebugs and Cotton Stainers ({{Hemiptera}}, {{Pyrrhocoridae}})},
