@article{macdonaldGeneticMetabolicDeterminants2011,
  title = {Genetic and Metabolic Determinants of Nutritional Phenotype in an Insect-Bacterial Symbiosis: {{PHENOTYPE OF INSECT}}-{{BACTERIAL SYMBIOSIS}}},
  shorttitle = {Genetic and Metabolic Determinants of Nutritional Phenotype in an Insect-Bacterial Symbiosis},
