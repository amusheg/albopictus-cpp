@article{mogiAnalysesNorthernDistributional2012,
  title = {Analyses of the {{Northern Distributional Limit}} of {{{\emph{Aedes}}}}{\emph{ Albopictus}} ({{Diptera}}: {{Culicidae}}) {{With}} a {{Simple Thermal Index}}},
