  shorttitle = {Effects of {{Constant}} and {{Fluctuating Low Temperatures}} on the {{Development}} of {{Aedes}} Aegypti ({{Diptera}}},
  author = {De Majo, Mar{\'i}a Sol and Zanotti, Gabriela and Campos, Ra{\'u}l E and Fischer, Sylvia},
  year = {2019},
  month = may,
  pages = {tjz087},
  issn = {0022-2585, 1938-2928},
  doi = {10.1093/jme/tjz087},
  abstract = {Most studies of the effects of low temperature on the development of immature stages of Aedes aegypti (L.) have been performed at constant temperatures in the laboratory, which may not accurately reflect the variable environmental conditions in the field. Thus, the aim of this study was to assess the effect of constant temperatures (CT) and fluctuating low temperatures (FT) on the fitness of Ae. aegypti of Buenos Aires, Argentina. Three CT treatments (12, 14, and 16\textdegree{}C) and three FT treatments (12, 14, and 16\textdegree{}C {$\pm$} 4\textdegree{}C) were performed and then survival, development time, and size of adults analyzed for each treatment. The immature stages completed development in all the treatments, with an average survival of 88\% at 16\textdegree{}C, 85\% at 14\textdegree{}C, and 22\% at 12\textdegree{}C, and showed no differences between the CT and FT treatments. Development times were similar between the CT and FT treatments at 16\textdegree{}C (average {$\pm$} SD: 22.7 {$\pm$} 2.0 d) and at 14\textdegree{}C (average {$\pm$} SD: 30.5 {$\pm$} 2.5 d), whereas at 12\textdegree{}C, they lasted longer under CT (average {$\pm$} SD: 46.6 {$\pm$} 5.1 d) than under FT (average {$\pm$} SD: 37 {$\pm$} 6.5 d). The sizes of the adults at 12 and 14\textdegree{}C were similar but larger than those at 16\textdegree{}C, and showed no differences between the CT and FT treatments. Compared to populations of other geographical regions assessed in previous studies, the shorter development times and the high survival at 14 and 16\textdegree{}C, and the ability to complete development at 12\textdegree{}C, a fact not previously reported, suggest that the Ae. aegypti population of Buenos Aires city has a higher tolerance to these conditions.},
  file = {/Users/sashamushegian/Zotero/storage/F6QURK63/De Majo et al. - 2019 - Effects of Constant and Fluctuating Low Temperatur.pdf},
  journal = {Journal of Medical Entomology},
  language = {en}
}

