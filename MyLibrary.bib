@article{liautaudSuperorganismsLooseCollections2019,
  title = {Superorganisms or Loose Collections of Species? {{A}} Unifying Theory of Community Patterns along Environmental Gradients},
  shorttitle = {Superorganisms or Loose Collections of Species?},
  author = {Liautaud, Kevin and {van Nes}, Egbert H. and Barbier, Matthieu and Scheffer, Marten and Loreau, Michel},
