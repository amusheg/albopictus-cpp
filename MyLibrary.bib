  author = {Bennett, Kelly L. and G{\'o}mez Mart{\'i}nez, Carmelo and Almanza, Alejandro and Rovira, Jose R. and McMillan, W. Owen and Enriquez, Vanessa and Barraza, Elia and Diaz, Marcela and {Sanchez-Galan}, Javier E. and Whiteman, Ari and Gittens, Rolando A. and Loaiza, Jose R.},
  year = {2019},
  month = dec,
  volume = {12},
  pages = {264},
  issn = {1756-3305},
  doi = {10.1186/s13071-019-3522-8},
  abstract = {Background:\hspace{0.6em} The long-distance dispersal of the invasive disease vectors Aedes aegypti and Aedes albopictus has intro-duced arthropod-borne viruses into new geographical regions, causing a significant medical and economic burden. The used-tire industry is an effective means of Aedes dispersal, yet studies to determine Aedes occurrence and the factors influencing their distribution along local transport networks are lacking. To assess infestation along the primary transport network of Panama we documented all existing garages that trade used tires on the highway and surveyed a subset for Ae. aegypti and Ae. albopictus. We also assess the ability of a mass spectrometry approach to classify mos-quito eggs by comparing our findings to those based on traditional larval surveillance.
Results:\hspace{0.6em}Both Aedes species had a high infestation rate in garages trading used tires along the highways, providing a conduit for rapid dispersal across Panama. However, generalized linear models revealed that the presence of Ae. aegypti is associated with an increase in road density by a log-odds of 0.44 (0.73\,{$\pm$}\,0.16; P\,=\,0.002), while the presence of Ae. albopictus is associated with a decrease in road density by a log-odds of 0.36 (0.09\,{$\pm$}\,0.63; P\,=\,0.008). Identifica-tion of mosquito eggs by mass spectrometry depicted similar occurrence patterns for both Aedes species as that obtained with traditional rearing methods.
Conclusions:\hspace{0.6em} Garages trading used tires along highways should be targeted for the surveillance and control of Aedes-mosquitoes and the diseases they transmit. The identification of mosquito eggs using mass spectrometry allows for the rapid evaluation of Aedes presence, affording time and cost advantages over traditional vector surveil-lance; this is of importance for disease risk assessment.},
  file = {/Users/sashamushegian/Zotero/storage/V8WDWEHI/Bennett et al. - 2019 - High infestation of invasive Aedes mosquitoes in u.pdf},
  journal = {Parasites \& Vectors},
