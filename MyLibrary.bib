@article{snell-roodMechanismsPlasticRescue2018,
  title = {Mechanisms of {{Plastic Rescue}} in {{Novel Environments}}},
  author = {{Snell-Rood}, Emilie C. and Kobiela,, Megan E. and Sikkink,, Kristin L. and Shephard, Alexander M.},
  year = {2018},
