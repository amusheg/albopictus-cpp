@article{kunoRevisitingHoustonMemphis2012,
  title = {Revisiting {{Houston}} and {{Memphis}}: {{The Background Histories Behind}} the {{Discovery}} of the {{Infestation}} by {{{\emph{Aedes}}}}{\emph{ Albopictus}} ({{Diptera}}: {{Culicidae}}) in the {{United States}} and {{Their Significance}} in the {{Contemporary Research}}},
