@article{redmondLinkedreadSequencingIdentifies2020,
  title = {Linked-Read Sequencing Identifies Abundant Microinversions and Introgression in the Arboviral Vector {{Aedes}} Aegypti},
  author = {Redmond, Seth N. and Sharma, Atashi and Sharakhov, Igor and Tu, Zhijian and Sharakhova, Maria and Neafsey, Daniel E.},
  year = {2020},
