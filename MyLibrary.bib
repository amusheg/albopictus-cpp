@article{rudmanMicrobiomeCompositionShapes2019,
  title = {Microbiome Composition Shapes Rapid Genomic Adaptation of {{{\emph{Drosophila}}}}{\emph{ Melanogaster}}},
  author = {Rudman, Seth M. and Greenblum, Sharon and Hughes, Rachel C. and Rajpurohit, Subhash and Kiratli, Ozan and Lowder, Dallin B. and Lemmon, Skyler G. and Petrov, Dmitri A. and Chaston, John M. and Schmidt, Paul},
